     // Directories that can be imported by a cc_* module generated_headers property
     export_include_dirs: [
         "usr/audio/include/uapi",
@@ -31,10 +28,6 @@ arrow_generator {
         "usr/include/audio/include/uapi",
         "usr/techpack/audio/include"
     ],
